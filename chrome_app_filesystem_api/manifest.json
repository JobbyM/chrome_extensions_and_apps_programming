{
  "app":{
    "background": {
      "scripts": ["background.js"]
    }
  },
  "manifest_version": 2,
  "name": "Chrome App FileSystem API",
  "description": "Chrome App FileSystem API",
  "version": "1.0",
  "icons": {
    "16":"images/icon16.png",
    "48":"images/icon48.png",
    "128":"images/icon128.png"
  },
  "permissions": [
    {"fileSystem": ["write", "directory"]}
  ]
}
